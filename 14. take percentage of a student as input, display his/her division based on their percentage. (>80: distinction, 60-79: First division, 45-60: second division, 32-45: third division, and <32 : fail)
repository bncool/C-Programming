#include <stdio.h>
#include <math.h>
int main () {
float per;
printf("enter your percentage");
scanf("%f",&per);
if (per>80) {printf("it is a distincation");
}
else if (per>60 && per<79) {printf("it is a first divison");
}
else if (per>45 && per<60) {printf("it is a second division");
}
else if (per>32 && per<45) {printf("it is a third division");
}


else {
printf("fail");
}
return 0;
}
