#include <stdio.h>
# include <math.h>
int main () {
	int year;
	printf("enter the  year ");
	scanf("%d",&year);
	
	if (year % 400 == 0) { printf("\n  it is a  is leaf year");
	
	}
	else if (year % 100== 0) { printf(" \n it  is not leaf year");
	}
	
	else if (year % 4 == 0) {printf("\n it  is not leaf year");
	}
	else { printf("\n year is not year");
	}
	
	
	return 0;
}
