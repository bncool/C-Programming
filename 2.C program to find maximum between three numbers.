#include <stdio.h>
#include <math.h>
int main() {
	int a,b,c; 
	printf("enter any three  number");
	scanf("%d%d%d",&a,&b,&c);
	if  (b>a && b>c) {
		printf("%d is the maximum",b);
	}
	 else if (a>b && a>c) {
	 	printf("%d is the maximum",a);
	 }
	 else {printf("%d is the maximum",c);
	 }
	
	return 0;
} 
